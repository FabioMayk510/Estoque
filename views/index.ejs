                            <td><%=dados[i].gaveta%></td>
                            <td><%=dados[i].traseira%></td>
                            <td><%=dados[i].frontal%></td>
                        </tr>
                    <% } %>
                    
                </tr>

            </table>
        </div>
</body>
</html>